
<!DOCTYPE html>
<html>
    <head>
        <title></title>
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">

        <script src="avalon.js"></script>

        <script >
          //  alert(document.lastModified)
            var model = avalon.define("performance", function(vm) {
                vm.langtext = ""
                vm.array = ["aaa", "bbb", "ccc"]
                vm.lang = []
                vm.range=50
                vm.test = '<span>444</span>';
                vm.h = "<li>ddd</li>"
            })

            model.lang.$watch("length", function() {
                model.langtext = model.lang.join(",")
            })

            setTimeout(function() {
                model.range = 90
                model.test = '<span>666</span>';
            }, 2000)
        </script>
        <script>

        </script>
    </head>
    <body>
        <fieldset ms-controller="performance">
            <div  ms-each="array">
                <div><input type="checkbox" ms-duplex="lang" ms-value="el">{{el}}</div>
            </div>
            <div>{{langtext}}</div>
            <input type="range" min="0" max="100" ms-duplex="range"/>

            {{test|html}}
            <ul ms-html="h"></ul>
        </fieldset>


    </body>
</html>
