<!DOCTYPE HTML>
<html id="html">
    <head>
        <meta charset="utf-8">
        <title>测试用例-yuewang</title>
    </head>
    <body>
        <div ms-controller="test">
            <button ms-click="one">测试1</button>
        </div>

        <script src="avalon.js"></script>
        <script>
            var data = {a: 1, b: 2};
            var model = avalon.define("test", function(vm) {
                vm.test1 = {};
                vm.test2 = {c: 3};

                vm.test1 = data;
                vm.one = function() {
                    // 假设数据是异步获取的，有没有办法赋值
                    setTimeout(function() {

                        model.test2 = data;
                    //    console.log(model)
                        alert('可以取到值：' + model.test1.a);
                        alert('无法取到值：' + model.test2.b);
                    }, 100);
                };
            });
            console.log(model)
        </script>
    </body>
</html>