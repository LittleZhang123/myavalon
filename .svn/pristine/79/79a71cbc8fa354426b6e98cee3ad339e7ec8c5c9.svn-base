<!DOCTYPE HTML>
<html id="html">
    <head>
        <meta charset="utf-8">
        <title>测试用例-yuewang</title>
    </head>
    <body>
        <div ms-controller="test">
            <button ms-click="one">测试1</button>
            <br>{{test1}}
        </div>

        <script src="avalon.mobile.js"></script>
        <script>
            avalon.ready(function() {
                var vm = avalon.define("test", function(vm) {
                    vm.test1 = '点击测试按钮没反应 绑定失败';
                });
                vm.one = function() {
                    vm.test1 = '绑定成功';
                };
                avalon.scan(document.body, vm);
            })

        </script>
    </body>
</html>