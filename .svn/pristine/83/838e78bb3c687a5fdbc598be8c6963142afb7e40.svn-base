<!DOCTYPE HTML>
<html id="html">
<head>
  <meta charset="utf-8">
  <title>测试用例-yuewang</title>
</head>
<body>
  <div ms-controller="test">
    <button ms-click="one">测试1</button>
  </div>

  <script src="avalon.js"></script>
  <script>
  window.str = 'ie8以下浏览器';
 var a = avalon.define("test", function(vm) {
    vm.str = '非ie浏览器';
    vm.msg = function() {
      // ie浏览器下this指向window
      alert(this.str);
    };
    vm.one = function() {

      vm.msg();
    };
  });

  </script>
</body>
</html>